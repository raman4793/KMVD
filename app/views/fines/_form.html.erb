<%= simple_form_for @fine, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :reason %>
  <%= f.input :amt %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                fines_path, :class => 'btn btn-default' %>
<% end %>
