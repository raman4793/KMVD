<%- model_class = VehicleFine -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:vehicle_id) %>:</strong></dt>
  <dd><%= @vehicle_fine.vehicle_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fine_id) %>:</strong></dt>
  <dd><%= @vehicle_fine.fine_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              vehicle_fines_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_vehicle_fine_path(@vehicle_fine), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              vehicle_fine_path(@vehicle_fine),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
