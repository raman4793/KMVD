<%= simple_form_for @parking, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.association :spot %>
  <%= f.association :vehicle %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                parkings_path, :class => 'btn btn-default' %>
<% end %>
